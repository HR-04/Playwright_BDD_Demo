{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",   // Ensure this is set to commonjs
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src"
    },
    "include": ["src/**/*.ts", "steps/**/*.ts","htmlReportGenerator.js"],
    "exclude": ["node_modules"]
  }
  